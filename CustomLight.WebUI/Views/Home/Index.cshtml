@{
	Layout = "~/Views/_Layouts/Main.cshtml";
}

<div>

	<table class="table table-responsive table-striped table-hover table-condensed">
		@foreach (CustomLight.Domain.Entities.Category item in Model)
		{
			<tr>
				<td>@item.Id</td>
				<td>@item.Name</td>
				<td>@item.Created</td>
				<td>@item.Updated</td>
				<td>@item.Description</td>
				<td>@item.ShortDescription</td>
			</tr>
		}
	</table>
</div>

